            <Rectangle Height="80" Canvas.Left="10" Stroke="#FF969696" Canvas.Top="10" Width="5" Fill="#FF969696"/>
            <Rectangle Height="5" Canvas.Left="11.5" Stroke="#FF969696" Canvas.Top="10" Width="80" Fill="#FF969696"/>
            <Rectangle Height="60" Canvas.Left="18" Stroke="#FF646464" Canvas.Top="18" Width="3" Fill="#FF646464" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="3" Canvas.Left="18.5" Stroke="#FF646464" Canvas.Top="18" Width="60" Fill="#FF646464" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="100" Stroke="White" Width="7" Fill="White" HorizontalAlignment="Center" VerticalAlignment="Center" Canvas.Bottom="0"/>
            <Rectangle Height="7" Stroke="White" Width="100" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="0" Canvas.Bottom="0"/>
            <Rectangle Height="80" Canvas.Left="10" Stroke="#FF969696" Canvas.Bottom="10" Width="5" Fill="#FF969696" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Rectangle Height="5" Canvas.Left="10" Stroke="#FF969696" Canvas.Bottom="10" Width="80" Fill="#FF969696" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="3" Canvas.Left="18" Stroke="#FF646464" Canvas.Bottom="18" Width="60" Fill="#FF646464" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Rectangle Height="60" Canvas.Left="18" Stroke="#FF646464" Canvas.Bottom="18" Width="3" Fill="#FF646464" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="7" Stroke="White" Width="100" Fill="White" Canvas.Right="0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Rectangle Height="100" Stroke="White" Width="7" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Center" Canvas.Right="0"/>
            <Rectangle Height="5" Canvas.Right="10" Stroke="#FF969696" Canvas.Top="10" Width="80" Fill="#FF969696" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="80" Canvas.Right="10" Stroke="#FF969696" Canvas.Top="10" Width="5" Fill="#FF969696" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="3" Canvas.Right="18" Stroke="#FF646464" Canvas.Top="18" Width="60" Fill="#FF646464" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Rectangle Height="60" Canvas.Right="18" Stroke="#FF646464" Canvas.Top="18" Width="3" Fill="#FF646464" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="80" Canvas.Right="10" Stroke="#FF969696" Canvas.Bottom="10" Width="5" Fill="#FF969696" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Rectangle Height="7" Stroke="White" Width="100" Fill="White" Canvas.Right ="0" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Bottom="0"/>
            <Rectangle Height="5" Canvas.Right="10" Stroke="#FF969696" Canvas.Bottom ="10" Width="80" Fill="#FF969696" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="60" Canvas.Right="18" Stroke="#FF646464" Canvas.Bottom ="18" Width="3" Fill="#FF646464" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="3" Canvas.Right="18" Stroke="#FF646464" Canvas.Bottom="18" Width="60" Fill="#FF646464" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="100" Stroke="White" Width="7" Fill="White" Canvas.Right="0" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Bottom="0"/>
        </Canvas>
    </Grid>
</UserControl>
